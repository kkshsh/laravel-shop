{
    "name": "simple-shop/laravel-shop",
    "description": "laravel商品系统",
    "keywords": ["Commodity", "shop", "laravel"],
    "license": "MIT",
    "homepage": "https://github.com/wangzhoudong/laravel-shop.git",
    "authors": [
        {
            "name": "Wangzd",
            "email": "wangzhoudong@foxmail.com"
        },
	    {
            "name": "JiangZhiHeng",
            "email": "j5110021@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "simple-shop/repositories": "master",
        "simple-shop/laravel-shop-cate": "master",
        "simple-shop/laravel-shop-brand": "master",
        "simple-shop/laravel-shop-store": "master",
        "simple-shop/laravel-shop-attribute": "master"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.0",
        "phpspec/phpspec": "^2.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "SimpleShop\\Commodity\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimpleShop\\Commodity\\CommodityServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
